{
  "multipart": [
    {
      "when": {
        "axis": "x"
      },
      "apply": {
        "model": "vanilla_plus:block/stripped_hollow_crimson_stem_horizontal",
        "y": 90
      }
    },
    {
      "when": {
        "mossy": "true",
        "axis": "x"
      },
      "apply": {
        "model": "vanilla_plus:block/hollow_log_mossy_x"
      }
    },
    {
      "when": {
        "axis": "y"
      },
      "apply": {
        "model": "vanilla_plus:block/stripped_hollow_crimson_stem"
      }
    },
    {
      "when": {
        "mossy": "true",
        "axis": "y"
      },
      "apply": {
        "model": "vanilla_plus:block/hollow_log_mossy_y"
      }
    },
    {
      "when": {
        "axis": "z"
      },
      "apply": {
        "model": "vanilla_plus:block/stripped_hollow_crimson_stem_horizontal"
      }
    },
    {
      "when": {
        "mossy": "true",
        "axis": "z"
      },
      "apply": {
        "model": "vanilla_plus:block/hollow_log_mossy_z"
      }
    }
  ]
}